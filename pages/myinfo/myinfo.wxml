          <text class="myinfo_title_bottom_number">7</text>
          <text class="myinfo_title_bottom_word">收藏</text>
        </view>
      </view>
    </view>
    <view class="myinfo_content">
      <view class="myinfo_content_assistant" hover-class="none" hover-stop-propagation="false">
        <text class="myinfo_content_assistant_title">求职助手</text>
        <text class="myinfo_content_assistant_other">其他/钱包</text>
      </view>
      <scroll-view class="myinfo_content_card_box" scroll-x="true">
        <view class="myinfo_content_card" hover-class="none" hover-stop-propagation="false">
          <view class="myinfo_content_card_left">
            <text class="myinfo_content_card_left_top">简历刷新</text>
            <text class="myinfo_content_card_left_bottom">提升简历活跃</text>
          </view>
          <view class="myinfo_content_card_right">
            <image class="myinfo_content_card_right_image" src="/images/icon/resume.png"></image>
          </view>
        </view>
        <view class="myinfo_content_card" hover-class="none" hover-stop-propagation="false">
          <view class="myinfo_content_card_left">
            <text class="myinfo_content_card_left_top">竞争力分析</text>
            <text class="myinfo_content_card_left_bottom">洞悉职业竞争</text>
          </view>
          <view class="myinfo_content_card_right">
            <image class="myinfo_content_card_right_image" src="/images/icon/doc.png"></image>
          </view>
        </view>
        <view class="myinfo_content_card" hover-class="none" hover-stop-propagation="false">
          <view class="myinfo_content_card_left">
            <text class="myinfo_content_card_left_top">职业测试</text>
