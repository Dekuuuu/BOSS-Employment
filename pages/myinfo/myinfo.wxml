          <text class="myinfo_title_user_name">周杰伦</text>
          <text class="myinfo_title_user_resume">我的在线简历</text>
        </view>
        <view class="myinfo_title_user_avatar" hover-class="none" hover-stop-propagation="false">
          <image class="myinfo_title_user_avatar_icon" src="/images/icon/avatar.png"></image>
        </view>
      </view>
      <view class="myinfo_title_bottom" hover-class="none" hover-stop-propagation="false">
        <view class="myinfo_title_bottom_box">
          <text class="myinfo_title_bottom_number">216</text>
          <text class="myinfo_title_bottom_word">沟通过</text>
